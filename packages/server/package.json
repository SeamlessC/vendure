{
    "name": "@vendure/server",
    "version": "1.8.1",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "run:server": "node ./dist/index.js",
        "start": "concurrently yarn:run*",
        "run:worker": "node ./dist/index-worker.js",
        "build": "rimraf dist && tsc -p ./tsconfig.json && cp -r ./email-plugin ./dist/server/",
        "populate": "node -r ts-node/register populate-dev-server.ts",
        "load-test:1k": "node -r ts-node/register load-testing/run-load-test.ts 1000",
        "load-test:10k": "node -r ts-node/register load-testing/run-load-test.ts 10000",
        "load-test:100k": "node -r ts-node/register load-testing/run-load-test.ts 100000"
    },
    "dependencies": {
        "nanoid": "^4.0.0",
        "crypto-js": "^4.1.1",
        "@vendure/admin-ui-plugin": "^1.8.1",
        "@vendure/asset-server-plugin": "^1.8.1",
        "@vendure/common": "^1.8.1",
        "@vendure/core": "^1.8.1",
        "@vendure/elasticsearch-plugin": "^1.8.1",
        "@vendure/email-plugin": "^1.8.1",
        "typescript": "4.3.5",
        "vendure-plugin-google-storage-assets": "5.0.1"
    },
    "devDependencies": {
        "@types/csv-stringify": "^3.1.0",
        "@vendure/testing": "^1.8.1",
        "@vendure/ui-devkit": "^1.8.1",
        "commander": "^7.1.0",
        "concurrently": "^5.0.0",
        "csv-stringify": "^5.3.3",
        "progress": "^2.0.3",
        "@types/crypto-js": "^4.1.1"
    }
}
